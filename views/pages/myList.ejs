<html>

<head>
    <title>Walmart Deals</title>
    <script src="https://code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
        crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.3.3/semantic.min.css" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.3.3/semantic.min.js"></script>
</head>

<body>

    <div class="ui secondary  menu">
        <a class="item" style="color:#1678c2; font-size:30px;" href="/">
            My Grocery List
        </a>
        <a class="item" href="/">
            Home
        </a>
        <div class="right menu">
            <div class="item">
                <div class="ui icon input">
                    <input type="text" placeholder="Search Walmart...">
                    <i class="search link icon"></i>
                </div>
            </div>
            <a class="ui item" href="test.html">
                Logout
            </a>
            <a class="ui item" href="test.html">
                Login/Signup
            </a>
        </div>
    </div>






    <div class="ui grid" style="margin-left:30px;">

        <% for(var i=0; i < result.rows.length; i++) { %>
            <div class="three wide column">
                <div class="ui card">
                    <div class="image">
                        <img src="https://i5.walmartimages.com/asr/209bb8a0-30ab-46be-b38d-58c2feb93e4a_1.1a15fb5bcbecbadd4a45822a11bf6257.jpeg?odnHeight=180&odnWidth=180&odnBg=FFFFFF">
                    </div>
                    <div class="content">
                        <a class="header">
                            <%=result.rows[i].name%>
                        </a>
                        <div class="description"> $
                            <%=result.rows[i].price%>
                        </div>
                    </div>
                </div>
            </div>
            <% } %>


    </div>

</body>

</html>